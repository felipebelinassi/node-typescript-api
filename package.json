{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "NodeJS TypeScript API",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "prestart": "yarn build",
    "start": "node dist/src/server.js",
    "start:dev": "ts-node-dev --respawn src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check src/**/*.ts test/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts test/**/*.ts"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.11.8",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.11.0",
    "jest": "^26.5.3",
    "prettier": "^2.1.2",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felipebelinassi/node-typescript-api.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Felipe Belinassi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/felipebelinassi/node-typescript-api/issues"
  },
  "homepage": "https://github.com/felipebelinassi/node-typescript-api#readme"
}
